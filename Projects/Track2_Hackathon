// Velo API Reference: https://www.wix.com/velo/reference/api-overview/introduction

$w.onReady(function () {

// Write your Javascript code here using the Velo framework API

//elements on this page
    $w('#googleMaps2')
	$w('#rsvpRadio');
	$w('#inviteTitle');
	$w('#quote');
	$w('#selling')
	$w('#location');
	

//get location info

	let saleLocation = $w('#googleMaps2').location;

	let saleLatitude = saleLocation.latitude;
	let saleLongitude = saleLocation.longitude;
	let saleLocationDescription = saleLocation.description;

	//get rsvp info

	let rsvpOptions = $w('#rsvpRadio').options;

	let firstValue = rsvpOptions[0].value;
	let secondValue = rsvpOptions[1].value;

//keeep track of people who are coming
	

//to make sure info is correct
	console.log(saleLocationDescription);
	console.log(saleLatitude);
	console.log(saleLongitude);
	console.log(firstValue);
	console.log(secondValue);





});
